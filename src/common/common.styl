/* 公用全局样式 */
body
  .page-body
    padding 16px
  .page-body-24
    padding 24px
  .page-title
    font-size 18px
    color rgba(0,0,0,.85)
    font-weight 700
    .border
      color rgba(0,0,0,0.25)
      padding 0 16px
    .title-border
      display inline-block
      width 4px
      height 18px
      vertical-align -3px
      background-color #5182e4
      border-radius 2px
      margin-right 8px
    .title-right
      float right
      font-size 14px
    &.title-border-bottom
      padding-bottom 16px
      border-bottom #f0f0f0 1px solid
  .border-bottom
    padding-bottom 16px
    border-bottom #f0f0f0 1px solid
  .page-searchForm
    padding 16px 0 0px
    margin-bottom 16px
    border-bottom #f0f0f0 1px solid
  .page-tableAction
    padding-bottom 16px
    .data-import-table
      a
        display inline-block
        line-height 32px
        padding-left 16px
        i
          padding-right 4px

  .ide-leftNav-gray
    .ide-leftMenu
      background-color #f9f9f9
      border-right 0
      .ant-menu-item
        &:hover
          background-color #eee
          color rgba(0,0,0,.75)
        &:after
          border-right 0
          transition initial
      .ant-menu-item-selected
        background-color #fff
        &:hover
          background-color #fff
          color #108ee9

  .ant-collapse
    >.ant-collapse-item
      >.ant-collapse-header
        padding 8px 0 8px 32px
        .arrow
          line-height 40px
          transform scale(0.75) rotate(0)
        &[aria-expanded=true] .arrow
          transform scale(0.75) rotate(90deg)
  .ant-steps
    .ant-steps-item-icon
      width 28px
      height 28px
      line-height 28px
      font-size 14px
      border-radius 50%
  .ant-time-picker
    width 107px

