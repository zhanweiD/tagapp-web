.show-label
  height 100% 
  .label-header
    display flex
    justify-content space-between
    .search-df
      display flex
      font-size 12px
      color rgba(0,0,0,.65)

  .label-main
    margin 16px
    min-height calc(100vh - 139px)
    
    .label-sider
      background #fff
      .sidebar-header
        font-size 14px
        height 45px
        line-height 45px
        padding-left 24px
        box-sizing border-box
        border-bottom 1px solid #f0f0f0
      .basicBox
        word-wrap break-word
        word-break normal
        .basicName
          color rgba(0,0,0,.45)

    .label-content
      height 100%
      background #fff
      margin-left 16px
      .label-tab
        height 100%
        .ant-tabs-nav-list
          padding-left 24px
          .ant-tabs-tab
            font-size 14px
        .ant-tabs-content-holder
          height 100%
          .ant-tabs-content
            height 100%
        .label-btn
          background-color rgba(32,150,255,.15)
          border 0
          border-radius 16px
          margin 8px 8px 8px 0px
          height 32px
          padding 4px 15px
          $:hover
            background-color #2096ff
            box-shadow 0px 2px 6px 0px rgba(99,157,209,1)
            border-radius 16px
            color #fff
.ant-progress-text
  color #fff
.h-100
  height 100%
.h-97
  height 97%